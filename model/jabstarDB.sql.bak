/*CREATE (EMPTY) TABLE TRIPS*/

CREATE TABLE JSTRIPS (
TP_DEP		VARCHAR(30),
TP_DEST		VARCHAR(30),
TP_BEAR		INT(3),
TP_DIST		INT(4),
TP_RANGE	INT(3),
PT_NAME		VARCHAR(30),
PRIMARY KEY (TP_DEP, TP_DEST, TP_RANGE)
);

/*CREATE (EMPTY) TABLE PILOTS*/

CREATE TABLE JSPILOTS (
PT_NAME		VARCHAR(30),
PT_PWORD	VARCHAR(30),
PRIMARY KEY (PT_NAME)
);

/*IMPLEMENT THE RELATIONSHIP 'PLANS'*/

ALTER TABLE 	JSTRIPS
ADD FOREIGN KEY (PT_NAME) REFERENCES JSPILOTS(PT_NAME); 